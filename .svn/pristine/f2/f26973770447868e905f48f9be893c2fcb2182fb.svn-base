package edu.southalabama.csc527.smallworld.model;

public class Stats {
	
		int f_HP;
		int f_MP;
		int f_Str;
		int f_Def;
		int f_Int;
		int f_Hit;

		public Stats(int HP,int MP,int Str,int Def,int Int,int Hit)
		{
			f_HP=HP;
			f_MP=MP;
			f_Str=Str;
			f_Def=Def;
			f_Int=Int;
			f_Hit=Hit;
		
		}
		public int getHP(){
		return f_HP;
		}

		public void setHP(int newHP){
		f_HP = newHP;
		}
		
		public int getMP(){
		return f_MP;
		}

		public void setMP(int newMP){
		f_MP = newMP;
		}
		
		public int getStr(){
		return f_Str;
		}

		public void setStr(int newStr){
		f_Str = newStr;
		}
		
		public int getDef(){
		return f_Def;
		}

		public void setDef(int newDef){
		f_Def = newDef;
		}
		
		public int getInt(){
		return f_Int;
		}

		public void setInt(int newInt){
		f_Int = newInt;
		}
		
		public int getHit(){
		return f_Hit;
		}

		public void setHit(int newHit){
		f_Hit = newHit;
		}
		}


